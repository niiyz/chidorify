{
  "name": "chidorify",
  "version": "1.0.2",
  "description": "chidorify is japanese check pattern maker",
  "main": "lib/chidorify.js",
  "scripts": {
    "build": "$(npm bin)/babel src --out-dir lib --ignore example",
    "build:example": "$(npm bin)/browserify src/example/example.js -t babelify -o public/js/bundle.js",
    "serve": "$(npm bin)/serve public",
    "test": "npm run build:example && npm run serve"
  },
  "dependencies": {
    "serve": "*"
  },
  "devDependencies": {
    "babel-runtime": "^6.9.2",
    "babel-cli": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-3": "^6.11.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "eslint": "^2.9.0"
  },
  "author": "niiyz",
  "license": "ISC"
}
